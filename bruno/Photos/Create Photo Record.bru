meta {
  name: Create Photo Record
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/photos
  body: json
  auth: inherit
}

body:json {
  {
    "bucket": "{{photoBucket}}",
    "key": "{{photoKey}}"
  }
}

script:post-response {
  const p = res.getBody()
  bru.setVar("photoId", p.id)
}

tests {
  test("should respond OK", function () {
    expect(res.getStatus()).to.equal(200);
  });
}
